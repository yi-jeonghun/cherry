<html>
<head>
	<title>Cherry</title>

	<%- include('common_resources.ejs') %>

	<script type="text/javascript" src="js/youtube_iframe_player.js"></script>

</head>
<body>
	<h1>Cherry</h1>

	<div id="id_div_music_list">

	</div>

	<div>
		<button type="button" id="id_btn_play">Play</button>
		<button type="button" id="id_btn_pause">Pause</button>
		<button type="button" id="id_btn_next">Next</button>
	</div>

	<div id="player" style="width: 100; height: 100; border: 1px solid black;"></div>


	<div id="id_div_duration">

	</div>

	<input id="id_slider" type="range" min="0" max="100" value="0" step="0.01" style="width: 300px;">


	<div>
		<span class="badge badge-primary" id="id_btn_seq_type" style="cursor: pointer;">Seq</span>
	</div>

	<div>
		<span class="badge badge-primary" id="id_btn_repeat_type" style="cursor: pointer;">All</span>
	</div>

</body>
</html>

<script type="text/javascript" src="js/kpop_control.js"></script>
