<!-- Modal Country Selector -->
<div class="modal fade" id="modal_choose_country" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="id_modal_choose_country" aria-hidden="true">
	<div class="modal-dialog modal-dialog-scrollable">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="id_modal_choose_country">Country</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body" id="id_div_country_list"></div>
		</div>
	</div>
</div>

<script type="text/javascript" src="/__cms/js/dj_selector.js"></script>
<script type="text/javascript" src="/__cms/js/country_selector.js"></script>

