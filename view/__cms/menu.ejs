		<a href="/__cms/user.vu">[User]</a>
		<a href="/__cms/era.vu">[Era]</a>
	</div>
	<div class="col-4 text-right d-flex">
		<div>
			<img id="id_img_playlist_country" data-toggle="modal" data-target="#modal_choose_country" style="width: 30px; border: solid 0.5px #bbbbbb; cursor: pointer;">
		</div>
		<div style="margin-right: 5px;">DJ</div>
		<div>
			<select class="form-control small" id="id_select_menu_dj_list" style="font-size: 0.8em;"></select>
		</div>
		<div class="ml-1">
			<span class="badge badge-sm" style="cursor: pointer;" id="id_btn_cms_dj_selector_fixed">Fixed</span>
			<span class="badge badge-sm" style="cursor: pointer;" id="id_btn_cms_dj_selector_random">Random</span>
			<span class="badge badge-sm border" style="cursor: pointer;" id="id_btn_cms_dj_selector_refresh">
				<i class="fas fa-sync-alt"></i>
			</span>
		</div>
	</div>
	<div class="col-3 text-right small">
		<%- include('../include/google_auth_html_cms.ejs') %>
		<%- include('../include/google_auth_js.ejs') %>
	</div>
